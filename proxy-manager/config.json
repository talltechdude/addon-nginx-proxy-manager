{
  "name": "Nginx Proxy Manager",
  "version": "dev",
  "slug": "nginxproxymanager",
  "description": "Manage Nginx proxy hosts with a simple, powerful interface",
  "url": "https://github.com/hassio-addons/addon-nginx-proxy-manager",
  "webui": "http://[HOST]:[PORT:81]",
  "startup": "application",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "services": ["mysql:need"],
  "ports": {
    "80": 80,
    "81": 81,
    "443": 443
  },
  "ports_description": {
    "80": "HTTP Entrance port",
    "81": "Proxy management web interface",
    "443": "HTTPS/SSL Entrance port"
  },
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": ["ssl:rw", "backup:rw"],
  "options": {
    "domain": "hassio.local",
    "homeassistant_url": "https://home.hassio.local",
    "vouch_url": "https://vouch.hassio.local",
    "jwt_max_age": 10080,
    "log_level": "info"
  },
  "schema": {
    "domain": "str",
    "homeassistant_url": "url",
    "vouch_url": "url",
    "jwt_max_age": "int",
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?"
  }
}
